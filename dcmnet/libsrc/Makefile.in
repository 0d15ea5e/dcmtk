#
#	Makefile for dcmnet
#


@SET_MAKE@

SHELL = /bin/sh
VPATH=@srcdir@:@top_srcdir@/include:@top_srcdir@/@configdir@/include
srcdir=@srcdir@
top_srcdir=@top_srcdir@
configdir=@top_srcdir@/@configdir@

dcmdatadir=$(top_srcdir)/../dcmdata

LOCALINCLUDES=-I$(dcmdatadir)/include -I$(includedir)/dcmdata

LOCALDEFS= 

include $(configdir)/@common_makefile@

objs= 	asccond.o assoc.o cond.o dcompat.o dimcancl.o \
	dimcmd.o dimcond.o dimdump.o dimecho.o dimfind.o \
	dimmove.o dimse.o dimstore.o diutil.o dulcond.o \
	dulconst.o dulextra.o dulfsm.o dulparse.o \
	dulpres.o dul.o lst.o lstcond.o


all: libdcmnet.a


install: all
	$(configdir)/mkinstalldirs $(libdir)
	$(INSTALL_DATA) libdcmnet.a $(libdir)/libdcmnet.a


clean:
	rm -f $(objs) 
	rm -f libdcmnet.a 
	rm -f $(TRASH)

distclean: clean
	rm -f $(DISTTRASH) 


libdcmnet.a: $(objs)
	$(AR) $(ARFLAGS) $@ $(objs)
	$(RANLIB) $@


# Dependencies

dependencies:
	$(CXX) -MM $(defines) $(includes) $(CPPFLAGS) $(CXXFLAGS) *.cc  > $(DEP)

$(objs): config.h


include $(DEP)





