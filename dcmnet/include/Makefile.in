#
#	Makefile for dcmnet/include
#


@SET_MAKE@

SHELL = /bin/sh
srcdir= @srcdir@
top_srcdir= @top_srcdir@
configdir=@top_srcdir@/@configdir@

include $(configdir)/@common_makefile@


all: 
	echo Nothing to do for all

install: 
	$(configdir)/mkinstalldirs $(includedir)/dcmnet
	for file in *.h ; do $(INSTALL_DATA) $$file $(includedir)/dcmnet ; done

clean:
	rm -f $(TRASH)

distclean:
	rm -f $(DISTTRASH)

dependencies:
	echo Nothing to do for dependencies



