#
#	Makefile for dcmwlm/libsrc
#


@SET_MAKE@

SHELL = /bin/sh
VPATH=@srcdir@:@top_srcdir@/include:@top_srcdir@/@configdir@/include
srcdir=@srcdir@
top_srcdir=@top_srcdir@
configdir=@top_srcdir@/@configdir@

ofstddir=$(top_srcdir)/../ofstd
dcmdatadir=$(top_srcdir)/../dcmdata
dcmnetdir=$(top_srcdir)/../dcmnet

LOCALINCLUDES=-I$(dcmdatadir)/include -I$(dcmnetdir)/include -I$(ofstddir)/include \
	-I$(includedir)/dcmdata -I$(includedir)/dcmnet -I$(includedir)/ofstd

LOCALDEFS= 

include $(configdir)/@common_makefile@

objs=wldbim.o wlds.o wldsdb.o wldsfs.o wlmactmg.o

LIB=libwrklstdb.$(LIBEXT)

all: $(LIB)

install: all
	$(configdir)/mkinstalldirs $(libdir)
	$(INSTALL_DATA) $(LIB) $(libdir)/$(LIB)


clean:
	rm -f $(objs) 
	rm -f $(LIB)
	rm -f $(TRASH)

distclean: clean
	rm -f $(DISTTRASH) 


$(LIB): $(objs)
	$(AR) $(ARFLAGS) $@ $(objs)
	$(RANLIB) $@


# Dependencies

dependencies:
	$(CXX) -MM $(defines) $(includes) $(CPPFLAGS) $(CXXFLAGS) *.cc  > $(DEP)

$(objs): config.h


include $(DEP)
