@SET_MAKE@

SHELL = /bin/sh

include @common_makefile@


all: 
	echo Nothing to do for all

install: 
	./mkinstalldirs $(includedir)
	$(INSTALL_DATA) include/osconfig.h $(includedir)
	$(INSTALL_DATA) include/config.h $(includedir)

clean:
	(cd include; rm -f $(TRASH))
	rm -f $(TRASH)

distclean:
	(cd include; rm -f $(DISTTRASH) config.h)
	rm -f $(DISTTRASH) Makefile.def Makefile.x 


dependencies:
	echo Nothing to do for dependencies



